pipeline {
  agent any
  stages {
    stage('Hello') {
      steps {
        echo 'Hello word'
      }
    }

    stage('Build') {
      steps {
        echo 'Construir Docker'
      }
    }

    stage('Runner') {
      steps {
        echo 'Correr Docker'
      }
    }
    stage('Inicializar') {
      steps {
        echo 'Cinicializar servidor'
      }
    }

  }
}